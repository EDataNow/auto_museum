
  <tbody>
      <tr>
        <td><%= chapter.position %></td>
        <td><%= chapter.title %></td>
        <td><%= chapter.video %></td>
        <td><%= chapter.pdf %></td>
        <td><%= chapter.audio %></td>
        <td><%= link_to 'Edit', edit_chapter_path(chapter) %></td>
        <td><%= link_to 'Delete', chapter, method: :delete, data: { confirm: 'Are you sure?' } %></td>
        <td>
          <%= link_to up_path(chapter_a: chapter), class: "btn btn-default" do %>
            &uarr;
          <% end %>
          <%= link_to down_path(chapter_a: chapter), class: "btn btn-default" do %>
            &darr;
          <% end %>
        </td>
      </tr>
  </tbody>
